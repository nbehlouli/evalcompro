<window  id="win" title="Compagne d'évaluation" apply="compagne.action.CompagneCreationAction" width="100%" height="100%" border="normal" contentStyle="overflow:auto"
 closable="true"  onClose="self.visible = false; event.stopPropagation();">
	<listbox id="admincomptelb" rows="1" model="@{winCtrl.model}"
		selectedItem="@{winCtrl.selected}"  width="100%" mold="paging" pageSize="8" height="300px" >
		<listhead sizable="true">
		
		    <listheader label="id compagne" width="180px" sort="auto" align="center"  visible="false"  />
			<listheader label="Nom compagne" width="180px" sort="auto" align="center" />
			<listheader label="Type compagne" width="180px" sort="auto" align="center" />
			<listheader label="Date debut compagne" width="180px" align="center" />
			<listheader label="Date fin compagne" width="180px" align="center" />
			<listheader label="id compate type " width="100px" align="center" visible="false" />
			
	
		</listhead>
		<listitem self="@{each=listcompagne}">
		    <listcell label="@{listcompagne.id_compagne}"  />
			<listcell label="@{listcompagne.nom_compagne}"  />
			<listcell label="@{listcompagne.type_compagne}" />
			<listcell label="@{listcompagne.date_deb_comp}"    />
			<listcell label="@{listcompagne.date_fin_comp}" />
			<listcell label="@{listcompagne.id_compagne_type}" />
			
			
		</listitem>
	</listbox>
	<grid span="true" sizedByContent="true" width="100%" height="130px">
        <auxhead>
            <auxheader colspan="13" class="topic">Edition des données</auxheader>
        </auxhead>
        <columns sizable="true">
            
            <column label="id compagne"  align="center" width="100px" visible="false"/>
            <column label="Nom compagne"  align="center" width="100px"/>
            <column label="Type compagne"  align="center" width="100px"/>
            <column label="Date debut compagne" align="center" width="100px"  />
            <column label="Date fin compagne" align="center" width="100px"/>
              <column label="id  compagne" align="center" width="100px" visible="false"/>
           
            
           
        </columns>
        <rows>
            <row>

				 <textbox id="idcompagne"  width="180px"  value="@{winCtrl.selected.id_compagne,save-when=none}"   visible="false"/>
                 <textbox id="nom"  width="180px"  value="@{winCtrl.selected.nom_compagne,save-when=none}" />
                 <listbox id="type_compagne"  width="180px"  mold="select" selectedItem="@{winCtrl.selected.type_compagne,save-when=none}" />
                 <datebox id="date_deb_comp"  cols="8"  value="@{winCtrl.selected.date_deb_comp,save-when=none}" format="yyyy/MM/dd"/>
                 <datebox id="date_fin_comp" cols="8" value="@{winCtrl.selected.date_fin_comp,save-when=none}" format="yyyy/MM/dd" />
                 <textbox id="id_compagne_type" width="180px"  value="@{winCtrl.selected.id_compagne_type,save-when=none}"  visible="false"/>
                
            </row>
        </rows>
    </grid>
	

	<hbox pack="center" width="100%" spacing="20">
		<div>
			<button id="add"  image="/image/DITEnabled.bmp" height="25px" width="30px" tooltiptext="Ajouter un utilisateur"/>
			<button id="okAdd"  image="/image/OkEnabled.bmp" height="25px" width="30px" tooltiptext="Valider"/>
			<button id="update" image="/image/EDITREDOEnabled.bmp" height="25px" width="30px" tooltiptext="Modifier un utilisateur"/>
			<button id="delete"  image="/image/DeleteEnabled.bmp" height="25px" width="30px" tooltiptext="Supprimer un utilisateur "/>
			<button id="effacer"  image="/image/CancelEnabled.bmp" height="25px" width="30px" tooltiptext="Retourner a la selection"/>
		</div>
		<div>

			
		</div>

		
	</hbox>
</window>